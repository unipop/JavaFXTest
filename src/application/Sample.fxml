<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="300.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.SampleController">
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="textFieldSample" />
            <TextArea fx:id="textAreaSample" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
            <Label fx:id="labelSample1" text="Label" GridPane.rowIndex="1" />
            <Label fx:id="labelSample2" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="buttonSample1" mnemonicParsing="false" onMouseClicked="#mouseClickedEvent1" text="Button1" GridPane.rowIndex="2" />
            <Button fx:id="buttonSample2" mnemonicParsing="false" onMouseClicked="#mouseClickedEvent2" text="Button2" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </center>
</BorderPane>
